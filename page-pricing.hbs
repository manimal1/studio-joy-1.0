{{!< default}}

<header class="main-header pricing tag-head no-cover">
    <nav class="fixed-nav overlay clearfix">
        {{#if @blog.navigation}}
            {{!--<a class="menu-button icon-menu" href="#"><span class="word">Menu</span></a>--}}
            {{navigation}}
        {{/if}}
    </nav>
</header>

<main class="pricing">
    <div class="row">
        <div class="small-12 medium-12 large-12">
            <h1 class="pricing__main-header">A Pricing Model for You</h1>
            <h2 class="pricing__model-header">Per Session</h2>
        </div>
    </div>

    <div class="row">
        <div class="small-12 medium-4 large-4">
            <div class="pricing__text card">
                <h4 class="pricing__text-title">First Session</h4>
                <h3 class="pricing__text-price">$10</h3>
                <p class="pricing__text-subtext">New Clients Only</p>
            </div>
        </div>
        <div class="small-12 medium-4 large-4">
            <div class="pricing__text card">
                <h4 class="pricing__text-title">One Session</h4>
                <h3 class="pricing__text-price">$25</h3>
            </div>
        </div>
        <div class="small-12 medium-4 large-4">
            <div class="pricing__text card">
                <h4 class="pricing__text-title">Ten Sessions</h4>
                <h3 class="pricing__text-price">$200</h3>
                <p class="pricing__text-subtext">6 month expiration</p>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="small-12 medium-12 large-12">
            <h2 class="pricing__model-header">Per Month</h2>
        </div>
    </div>

    <div class="row">
        <div class="small-12 medium-4 large-4">
            <div class="pricing__text card">
                <h4 class="pricing__text-title">8 Sessions / Month</h4>
                <h3 class="pricing__text-price">$149</h3>
            </div>
        </div>
        <div class="small-12 medium-4 large-4">
            <div class="pricing__text card">
                <h4 class="pricing__text-title">12 Sessions / Month</h4>
                <h3 class="pricing__text-price">$199</h3>
            </div>
        </div>
        <div class="small-12 medium-4 large-4">
            <div class="pricing__text card">
                <h4 class="pricing__text-title">16 Sessions / Month</h4>
                <h3 class="pricing__text-price">$249</h3>
                <p class="pricing__text-subtext">30 day expiration</p>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="small-12 medium-12 large-12">
            <h2 class="pricing__model-header">Contract Option</h2>
        </div>
    </div>

    <div class="row">
        <div class="small-12 medium-6 large-6 medium-offset-3 large-offset-3">
            <div class="pricing__text card card--membership">
                <h4 class="pricing__text-title">6 Month Membership</h4>
                <h3 class="pricing__text-price">10% Off per Month</h3>
                <div class="square button large">
                    <script src="https://widgets.healcode.com/javascripts/healcode.js" type="text/javascript"></script>

                    <healcode-widget data-version="0.2" data-link-class="healcode-login-register-text-link" data-site-id="28659" data-mb-site-id="314234" data-type="account-link" data-inner-html="Register"></healcode-widget>
                </div>
            </div>
        </div>
    </div>
</main>